{
  "name": "cs-reading-whitelist",
  "private": true,
  "scripts": {
    "start": "npm run dev -- --env open",
    "build": "webpack -c ./webpack.config.cjs --env target=production",
    "build:analyze": "npm run build -- --env analyze",
    "build:profiling": "npm run build -- --env profiling",
    "build:stats": "npm run build --profile --json > ./stats/webpack-bundle-stats.json",
    "deploy": "gh-pages -d ./dist/",
    "dev": "webpack serve -c ./webpack.config.cjs --env target=development",
    "dev:nocircular": "npm run dev -- --env nocircular",
    "dev:write": "npm run dev -- --env write",
    "lint": "npm-run-all --parallel lint:styles tsc",
    "lint:styles": "stylelint ./src/**/*.scss",
    "nodemon": "nodemon --config ./nodemon.json",
    "nodemon:build": "npm run nodemon -- build",
    "nodemon:dev": "npm run nodemon -- dev",
    "prettier:check": "prettier --check \"./**/*.(j|t)s?(x)\"",
    "prettier:write": "prettier --write \"./**/*.(j|t)s?(x)\"",
    "publish": "npm run lint:styles && npm run build && npm run deploy",
    "serve": "serve -s ./dist/",
    "test": "jest -c=./jest.config.cjs --passWithNoTests",
    "test:watch": "npm test -- --watch",
    "tsc": "tsc"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ie_mob 11",
    "not edge <= 18",
    "not op_mini all"
  ],
  "dependencies": {
    "@reduxjs/toolkit": "2.2.5",
    "clsx": "2.1.1",
    "lodash-es": "4.17.21",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-redux": "9.1.2",
    "react-router-dom": "6.23.1",
    "redux-saga": "1.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.24.5",
    "@babel/plugin-transform-runtime": "7.24.3",
    "@babel/preset-env": "7.24.5",
    "@babel/preset-react": "7.24.1",
    "@babel/preset-typescript": "7.24.1",
    "@babel/runtime-corejs3": "7.24.5",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.11",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/jest": "29.5.12",
    "@types/lodash-es": "4.17.12",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.19",
    "babel-jest": "29.7.0",
    "babel-loader": "9.1.3",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "circular-dependency-plugin": "5.2.2",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "12.0.2",
    "core-js": "3.37.1",
    "css-loader": "7.1.1",
    "css-minimizer-webpack-plugin": "6.0.0",
    "dotenv": "16.4.5",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "gh-pages": "6.1.1",
    "html-webpack-plugin": "5.6.0",
    "jest": "29.7.0",
    "mini-css-extract-plugin": "2.9.0",
    "nodemon": "3.1.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.38",
    "postcss-loader": "8.1.1",
    "prettier": "3.2.5",
    "react-refresh": "0.14.0",
    "sass": "1.77.2",
    "sass-loader": "14.2.1",
    "serve": "14.2.3",
    "stylelint-config-yialo": "0.5.1",
    "terser-webpack-plugin": "5.3.10",
    "typescript": "5.4.5",
    "webpack": "5.91.0",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1"
  },
  "engines": {
    "node": "20"
  },
  "author": "Aleksei Arro <polsck@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yialo/cs-reading-whitelist.git"
  }
}
