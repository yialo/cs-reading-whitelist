{
  "name": "cs-reading-whitelist",
  "private": true,
  "scripts": {
    "build": "npm run webpack -- --env target=production",
    "build:analyze": "npm run build -- --env analyze",
    "build:stats": "npm run build --profile --json > ./stats/webpack-bundle-stats.json",
    "deploy": "gh-pages -d ./dist/",
    "dev": "npm run webpack:serve -- --env target=development",
    "dev:nocircular": "npm run dev -- --env nocircular",
    "dev:write": "npm run dev -- --env write",
    "lint": "npm-run-all --parallel lint:styles lint:scripts typecheck",
    "lint:scripts": "eslint ./*.js ./src/**/*.{j,t}s?(x)",
    "lint:styles": "stylelint ./src/**/*.scss",
    "nodemon": "nodemon --config ./nodemon.json",
    "nodemon:build": "npm run nodemon -- build",
    "nodemon:dev": "npm run nodemon -- dev",
    "publish": "npm run lint:styles && npm run lint:scripts && npm run build && npm run deploy",
    "test": "jest -c=./jest.config.js --passWithNoTests",
    "test:watch": "npm test -- --watch",
    "typecheck": "tsc",
    "webpack": "webpack -c ./webpack.config.js",
    "webpack:serve": "webpack serve -c ./webpack.config.js"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ie_mob 11",
    "not edge <= 18",
    "not op_mini all"
  ],
  "dependencies": {
    "@reduxjs/toolkit": "1.9.3",
    "clsx": "1.2.1",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.5",
    "react-router-dom": "6.9.0",
    "redux": "4.2.1",
    "redux-devtools-extension": "2.13.9",
    "redux-saga": "1.2.3",
    "reselect": "4.1.7"
  },
  "devDependencies": {
    "@babel/core": "7.21.3",
    "@babel/plugin-transform-runtime": "7.21.0",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.21.0",
    "@babel/runtime-corejs3": "7.21.0",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.10",
    "@types/jest": "29.5.0",
    "@types/lodash": "4.14.191",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "autoprefixer": "10.4.14",
    "babel-jest": "29.5.0",
    "babel-loader": "9.1.2",
    "babel-plugin-transform-imports": "2.0.0",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "circular-dependency-plugin": "5.2.2",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "core-js": "3.29.1",
    "css-loader": "6.7.3",
    "css-minimizer-webpack-plugin": "4.2.2",
    "dotenv": "16.0.3",
    "eslint-config-yialo": "0.6.20",
    "fork-ts-checker-webpack-plugin": "8.0.0",
    "gh-pages": "5.0.0",
    "html-loader": "4.2.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "mini-css-extract-plugin": "2.7.5",
    "nodemon": "2.0.21",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.21",
    "postcss-loader": "7.1.0",
    "react-refresh": "0.14.0",
    "sass": "1.59.3",
    "sass-loader": "13.2.1",
    "stylelint-config-yialo": "0.5.0",
    "terser-webpack-plugin": "5.3.7",
    "typescript": "4.9.5",
    "webpack": "5.76.2",
    "webpack-bundle-analyzer": "4.8.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.13.1"
  },
  "engines": {
    "node": "18"
