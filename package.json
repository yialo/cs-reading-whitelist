{
  "name": "cs-reading-whitelist",
  "scripts": {
    "build": "npm run webpack -- --env target=production",
    "build:analyze": "npm run build -- --env analyze",
    "build:stats": "npm run build --profile --json > ./stats/webpack-bundle-stats.json",
    "deploy": "gh-pages -d ./dist/",
    "dev": "npm run webpack:serve -- --env target=development",
    "dev:tscheck": "npm run dev -- --env tscheck",
    "dev:write": "npm run dev -- --env write",
    "imagemin": "gulp imagemin -f ./scripts/imagemin.js",
    "lint": "npm run lint:styles && npm run lint:scripts && npm run lint:editorconfig && npm run typecheck",
    "lint:scripts": "eslint ./*.js ./scripts/**/*.js ./src/**/*.{j,t}s?(x)",
    "lint:styles": "stylelint ./src/**/*.scss",
    "nodemon": "nodemon --config ./nodemon.json",
    "nodemon:build": "npm run nodemon -- build",
    "nodemon:dev": "npm run nodemon -- dev",
    "publish": "npm run build && npm run deploy",
    "test": "jest -c=./jest.config.js",
    "tsc": "tsc",
    "webpack": "webpack -c ./webpack.config.js",
    "webpack:serve": "webpack serve -c ./webpack.config.js"
  },
  "author": "Aleksei Arro <polsck@gmail.com>",
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ie_mob 11",
    "not edge <= 18",
    "not op_mini all"
  ],
