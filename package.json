{
  "name": "cs-reading-whitelist",
  "scripts": {
    "build": "npm run webpack -- --env.target=production",
    "build:analyze": "npm run build -- --env.analyze",
    "build:stats": "webpack --config ./config/webpack.config.js --env.target=production --profile --json > ./stats/webpack-bundle-stats.json",
    "deploy": "gh-pages -d ./dist/",
    "dev": "npm run webpack-dev-server -- --env.target=development",
    "dev:tscheck": "npm run dev -- --env.tscheck",
    "dev:write": "npm run dev -- --env.write",
    "imagemin": "gulp imagemin -f ./scripts/imagemin.js",
    "lint": "npm run lint:scripts && npm run lint:styles",
    "lint:scripts": "eslint ./config/**/*.js ./src/**/*.{j,t}s?(x)",
    "lint:styles": "stylelint ./src/**/*.css",
    "nodemon": "nodemon --config ./config/nodemon.json",
    "nodemon:build": "npm run nodemon -- build",
    "nodemon:dev": "npm run nodemon -- dev",
    "test": "jest --config=./config/jest.config.js",
    "tscheck": "tsc",
    "webpack": "webpack --config ./config/webpack.config.js",
    "webpack-dev-server": "webpack-dev-server --config ./config/webpack.config.js"
  },
  "author": "Aleksei Arro <polsck@gmail.com>",
  "browserslist": [
    "defaults",
    "not ie 11",
