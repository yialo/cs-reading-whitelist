  "scripts": {
    "build": "npm run prod -- --env.deploy",
    "check-types": "tsc --project ./config/tsconfig.json",
    "dev": "npm run webpack-dev-server -- --env.purpose=development",
    "dev:debug": "npm run nodemon -- inspect:dev",
    "inspect": "node --inspect-brk ./node_modules/webpack/bin/webpack.js --config ./config/webpack.config.js",
    "inspect:dev": "npm run inspect -- --env.purpose=development",
    "inspect:prod": "npm run inspect -- --env.purpose=production",
    "lint": "npm run lint:scripts && npm run lint:styles",
    "lint:scripts": "eslint ./config/**/*.js ./src/**/*.{j,t}s?(x)",
    "lint:styles": "stylelint ./src/**/*.css",
    "nodemon": "nodemon --config ./config/nodemon.json",
    "nodemon:dev": "npm run nodemon -- dev",
    "nodemon:prod": "npm run nodemon -- prod",
    "prod": "npm run webpack -- --env.purpose=production",
    "prod:analyze": "npm run prod -- --env.analyze",
    "prod:debug": "npm run nodemon -- inspect:prod",
    "prod:stats": "webpack --config ./config/webpack.config.js --env.purpose=production --profile --json > ./stats/webpack-bundle-stats.json",
    "start": "npm run dev",
    "test": "jest --config=./config/jest.config.js",
    "webpack": "webpack --config ./config/webpack.config.js",
    "webpack-dev-server": "webpack-dev-server --config ./config/webpack.config.js"
  },
  "version": "0.6.8"
