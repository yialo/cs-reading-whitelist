{
  "name": "cs-reading-whitelist",
  "private": true,
  "scripts": {
    "start": "npm run dev -- --env open",
    "build": "webpack -c ./webpack.config.cjs --env target=production",
    "build:local": "webpack -c ./webpack.config.cjs --env target=production --env local",
    "build:analyze": "npm run build -- --env analyze",
    "build:profiling": "npm run build -- --env profiling",
    "build:stats": "npm run build --profile --json > ./stats/webpack-bundle-stats.json",
    "deploy": "gh-pages -d ./dist/",
    "dev": "webpack serve -c ./webpack.config.cjs --env target=development",
    "dev:nocircular": "npm run dev -- --env nocircular",
    "dev:write": "npm run dev -- --env write",
    "lint": "npm-run-all --parallel lint:styles tsc",
    "lint:styles": "stylelint ./src/**/*.scss",
    "nodemon": "nodemon --config ./nodemon.json",
    "nodemon:build": "npm run nodemon -- build",
    "nodemon:dev": "npm run nodemon -- dev",
    "prettier:check": "prettier --check \"./**/*.(j|t)s?(x)\"",
    "prettier:write": "prettier --write \"./**/*.(j|t)s?(x)\"",
    "publish": "npm run lint:styles && npm run build && npm run deploy",
    "serve": "http-server ./dist/",
    "test": "jest -c=./jest.config.cjs --passWithNoTests",
    "test:watch": "npm test -- --watch",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ie_mob 11",
    "not edge <= 18",
    "not op_mini all"
  ],
  "dependencies": {
    "@reduxjs/toolkit": "2.5.0",
    "clsx": "2.1.1",
    "lodash-es": "4.17.21",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-redux": "9.2.0",
    "react-router": "7.1.1",
    "redux-saga": "1.3.0"
  },
  "devDependencies": {
    "@awmottaz/prettier-plugin-void-html": "1.7.0",
    "@babel/core": "7.26.0",
    "@babel/plugin-transform-runtime": "7.25.9",
    "@babel/preset-env": "7.26.0",
    "@babel/preset-react": "7.26.3",
    "@babel/preset-typescript": "7.26.0",
    "@babel/runtime-corejs3": "7.26.0",
    "@ianvs/prettier-plugin-sort-imports": "4.4.0",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.15",
    "@teamsupercell/typings-for-css-modules-loader": "2.5.2",
    "@types/jest": "29.5.14",
    "@types/lodash-es": "4.17.12",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.20",
    "babel-jest": "29.7.0",
    "babel-loader": "9.2.1",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "circular-dependency-plugin": "5.2.2",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "12.0.2",
    "core-js": "3.40.0",
    "css-loader": "7.1.2",
    "css-minimizer-webpack-plugin": "7.0.0",
    "dotenv": "16.4.7",
    "gh-pages": "6.3.0",
    "html-webpack-plugin": "5.6.3",
    "http-server": "14.1.1",
    "jest": "29.7.0",
    "mini-css-extract-plugin": "2.9.2",
    "nodemon": "3.1.9",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.49",
    "postcss-loader": "8.1.1",
    "postcss-scss": "4.0.9",
    "prettier": "3.4.2",
    "react-refresh": "0.14.2",
    "sass-embedded": "1.83.1",
    "sass-loader": "16.0.4",
    "stylelint": "16.13.0",
    "stylelint-scss": "6.10.0",
    "terser-webpack-plugin": "5.3.11",
    "typescript": "5.7.3",
    "webpack": "5.97.1",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.0"
  },
  "engines": {
    "node": "22"
  },
  "author": "Aleksei Arro <polsck@gmail.com>",
